<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/routes"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.bestDate.fragment.StartFragment"
        android:label="StartFragment" >
        <action
            android:id="@+id/action_startFragment_to_onboardStartFragment"
            app:destination="@id/onboardStartFragment"
            app:popUpTo="@id/onboardStartFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/onboardStartFragment"
        android:name="com.bestDate.fragment.onboarding.OnboardStartFragment"
        android:label="OnboardStartFragment" >
        <action
            android:id="@+id/action_onboardStartFragment_to_onboardSecondFragment"
            app:destination="@id/onboardSecondFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_onboardStartFragment_to_authFragment"
            app:destination="@id/authFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/onboardSecondFragment"
        android:name="com.bestDate.fragment.onboarding.OnboardSecondFragment"
        android:label="OnboardSecondFragment" >
        <action
            android:id="@+id/action_onboardSecondFragment_to_authFragment"
            app:destination="@id/authFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/authFragment"
        android:name="com.bestDate.fragment.auth.AuthFragment"
        android:label="AuthFragment" >
        <action
            android:id="@+id/action_authFragment_to_passRecoveryFragment"
            app:destination="@id/passRecoveryFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_authFragment_to_startRegistrationFragment"
            app:destination="@id/startRegistrationFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/geoEnableFragment"
        android:name="com.bestDate.fragment.GeoEnableFragment"
        android:label="GeoEnableFragment" />
    <fragment
        android:id="@+id/passRecoveryFragment"
        android:name="com.bestDate.fragment.passRecovery.PassRecoveryFragment"
        android:label="PassRecoveryFragment" >
        <action
            android:id="@+id/action_passRecoveryFragment_to_passRecoveryOtpFragment"
            app:destination="@id/passRecoveryOtpFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/startRegistrationFragment"
        android:name="com.bestDate.fragment.registration.StartRegistrationFragment"
        android:label="StartRegistrationFragment" >
        <action
            android:id="@+id/action_startRegistrationFragment_to_authFragment"
            app:destination="@id/authFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_right"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_left" />
        <action
            android:id="@+id/action_startRegistrationFragment_to_continueRegistrationFragment"
            app:destination="@id/continueRegistrationFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/continueRegistrationFragment"
        android:name="com.bestDate.fragment.registration.ContinueRegistrationFragment"
        android:label="ContinueRegistrationFragment" >
        <action
            android:id="@+id/action_continueRegistrationFragment_to_registrationOtpFragment"
            app:destination="@id/registrationOtpFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/profilePhotoEditingFragment"
        android:name="com.bestDate.fragment.profilePhotoEditor.ProfilePhotoEditingFragment"
        android:label="ProfilePhotoEditingFragment" >
        <action
            android:id="@+id/action_profilePhotoEditingFragment_to_photoEditorFragment"
            app:destination="@id/photoEditorFragment"
            app:enterAnim="@anim/push_up_in"
            app:exitAnim="@anim/push_up_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_profilePhotoEditingFragment_to_questionnaireFragment"
            app:destination="@id/questionnaireFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/photoEditorFragment"
        android:name="com.bestDate.fragment.profilePhotoEditor.PhotoEditorFragment"
        android:label="PhotoEditorFragment" >
        <argument
            android:name="selectedImage"
            app:argType="com.bestDate.data.model.Image" />
    </fragment>
    <fragment
        android:id="@+id/questionnaireFragment"
        android:name="com.bestDate.fragment.questionnarie.QuestionnaireFragment"
        android:label="QuestionnaireFragment" >
        <action
            android:id="@+id/action_questionnaireFragment_to_geoEnableFragment"
            app:destination="@id/geoEnableFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/registrationOtpFragment"
        android:name="com.bestDate.fragment.registration.RegistrationOtpFragment"
        android:label="RegistrationOtpFragment" >
        <action
            android:id="@+id/action_registrationOtpFragment_to_profilePhotoEditingFragment"
            app:destination="@id/profilePhotoEditingFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="email"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/passRecoveryOtpFragment"
        android:name="com.bestDate.fragment.passRecovery.PassRecoveryOtpFragment"
        android:label="PassRecoveryOtpFragment" >
        <argument
            android:name="email"
            app:argType="string" />
        <action
            android:id="@+id/action_passRecoveryOtpFragment_to_passRecoverySetNewFragment"
            app:destination="@id/passRecoverySetNewFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/passRecoverySetNewFragment"
        android:name="com.bestDate.fragment.passRecovery.PassRecoverySetNewFragment"
        android:label="PassRecoverySetNewFragment" >
        <action
            android:id="@+id/action_passRecoverySetNewFragment_to_geoEnableFragment"
            app:destination="@id/geoEnableFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
</navigation>