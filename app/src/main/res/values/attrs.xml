<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StandardInput">
        <attr name="isPassField" format="boolean"/>
        <attr name="hint" format="string"/>
        <attr name="src" format="reference"/>
    </declare-styleable>

    <declare-styleable name="BaseButton">
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="UserDataButtonView">
        <attr name="button_title" format="string"/>
        <attr name="button_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DirectionLineButtonView">
        <attr name="button_name" format="string"/>
        <attr name="button_image" format="reference"/>
        <attr name="button_background" format="reference"/>
    </declare-styleable>

    <declare-styleable name="BackButtonView">
        <attr name="back_button_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ToolbarView">
        <attr name="toolbar_title" format="string"/>
        <attr name="toolbar_action_visible" format="boolean"/>
        <attr name="toolbar_line_visibility" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="NoDataViewWithLoading">
        <attr name="no_data_text" format="string"/>
        <attr name="start_with_loading" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="QuestionnaireTitleView">
        <attr name="questionnaire_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="QuestionnaireInfoView">
        <attr name="questionnaire_item_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="TitleDescriptionView">
        <attr name="title_description_title" format="string"/>
        <attr name="title_description_description" format="string"/>
    </declare-styleable>

    <declare-styleable name="SettingsInputView">
        <attr name="settings_input_hint" format="string"/>
        <attr name="settings_input_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SettingsInfoView">
        <attr name="settings_info_hint" format="string"/>
        <attr name="settings_info_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SettingsButtonView">
        <attr name="settings_button_title" format="string"/>
    </declare-styleable>
</resources>