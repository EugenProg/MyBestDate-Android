<?xml version="1.0" encoding="utf-8"?>
<com.bestDate.view.squareFrameLayout.SquareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.bestDate.view.LoadingDotsView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"/>

    <ImageView
        android:id="@+id/photo_thump"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <ImageView
        android:id="@+id/photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</com.bestDate.view.squareFrameLayout.SquareFrameLayout>