<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:background="@color/bg_main">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_marginStart="42dp"
            android:src="@drawable/circle_white"
            app:layout_constraintBottom_toTopOf="@+id/textViewFirst"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_marginEnd="42dp"
            android:src="@drawable/circle_grey"
            app:layout_constraintBottom_toTopOf="@+id/textViewFirst"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <View
            android:layout_width="0dp"
            android:layout_height="5.5dp"
            android:layout_marginHorizontal="7dp"
            android:background="@color/gray_787C7E"
            app:layout_constraintBottom_toTopOf="@+id/textViewFirst"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintStart_toStartOf="@+id/imageView8"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/textViewFirst"
            style="@style/component_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white_20"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView8"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="km" />

        <TextView
            android:id="@+id/textViewSecond"
            style="@style/component_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white_20"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="km" />

        <SeekBar
            android:id="@+id/bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxHeight="6dp"
            android:minHeight="6dp"
            android:paddingStart="48dp"
            android:paddingEnd="48dp"
            android:progressDrawable="@drawable/progress_tint_distance"
            app:layout_constraintBottom_toTopOf="@+id/textViewFirst"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>